{
  "name": "@4ev3rm0re/dev-proxy-server",
  "type": "module",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "version": "0.2.2",
  "exports": "./dist/cli.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "public"
  ],
  "bin": {
    "dev-proxy": "./dist/cli.js"
  },
  "license": "MIT",
  "scripts": {
    "dev": "tsx watch src/cli.ts",
    "build": "yarn run build:server && yarn run copy-client",
    "build:server": "tsc",
    "copy-client": "mkdir -p public && cp -r ../client/dist/* public/",
    "publish": "node scripts/publish.js",
    "clean": "rm -rf dist",
    "start": "node dist/index.js",
    "setup": "ts-node src/test-setup.ts"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "http-proxy-middleware": "^3.0.3",
    "node-json-db": "^2.3.0",
    "path-to-regexp": "^8.2.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.13",
    "prompts": "^2.4.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
