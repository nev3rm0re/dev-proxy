    "eslint-plugin-storybook": "^0.11.2",
    "globals": "^15.14.0",
    "postcss": "^8.4.49",
    "storybook": "^8.5.2",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
