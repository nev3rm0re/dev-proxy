    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.7",
    "@replit/codemirror-vim": "^6.2.1",
    "@shadcn/ui": "^0.0.4",
    "@tanstack/react-table": "^8.20.6",
